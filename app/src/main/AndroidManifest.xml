<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kaushiksamba.game" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StartingClass"
            android:label="@string/title_activity_starting_class"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".IntroScreen"
            android:label="@string/title_activity_intro_screen" >
        </activity>
        <activity
            android:name=".Instructions"
            android:label="@string/title_activity_instructions" >
        </activity>
        <activity
            android:name=".HighScore"
            android:label="@string/title_activity_high_score" >
        </activity>
        <activity
            android:name=".DifficultySelection"
            android:label="@string/title_activity_difficulty_selection">
        </activity>
    </application>

</manifest>
